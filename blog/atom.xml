---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Leto Labs Blog</title>
  <link href="{{site.root_uri}}/blog/atom.xml" rel="self"/>
  <link href="{{site.root_uri}}"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ site.root_uri }}/blog/</id>
  <author>
    <name>Jonathan "Duke" Leto</name>
  </author>
  {% for post in site.posts limit:20 %}
    <entry>
      <title>{{ post.title }}</title>
      <link href="{{ site.root_uri}}{{ post.url }}"/>
      <updated>{{ post.date | date_to_xmlschema }}</updated>
      <id>{{site.root_uri}}{{ post.id }}</id>
      <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
  {% endfor %}
</feed>
